<?xml version="1.0"?>
<package >
	<metadata>
		<id>Shuttle.Esb.NetMQ</id>
		<version>{semver}</version>
		<authors>Eben Roux</authors>
		<owners>Eben Roux</owners>
    <license type="expression">BSD-3-Clause</license>
		<projectUrl>https://github.com/shuttle/Shuttle.Esb.NetMQ</projectUrl>
		<iconUrl>https://raw.githubusercontent.com/Shuttle/Shuttle.Esb.NetMQ/master/.media/logo.png</iconUrl>
    <repository type="git" url="https://github.com/Shuttle/Shuttle.Esb.NetMQ.git" />
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>NetMQ implementation for use with Shuttle.Esb.</description>
		<releaseNotes/>
    <copyright>Copyright (c) {year}, Eben Roux</copyright>
		<tags>shuttle netmq zeromq zmq</tags>
    <dependencies>
      <group>
        <dependency id="NetMQ" version="{NetMQ-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
        <dependency id="Shuttle.Core.Container" version="{Shuttle.Core.Container-version}" />
        <dependency id="Shuttle.Core.Uris" version="{Shuttle.Core.Uris-version}" />
      </group>

      <group targetFramework="netstandard2.0">
        <dependency id="NetMQ" version="{NetMQ-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
        <dependency id="Shuttle.Core.Container" version="{Shuttle.Core.Container-version}" />
        <dependency id="Shuttle.Core.Uris" version="{Shuttle.Core.Uris-version}" />
        <dependency id="System.Configuration.ConfigurationManager" version="{System.Configuration.ConfigurationManager-version}" />
      </group>

      <group targetFramework="netcoreapp2.1">
        <dependency id="NetMQ" version="{NetMQ-version}" />
        <dependency id="Shuttle.Esb" version="{Shuttle.Esb-version}" />
        <dependency id="Shuttle.Core.Container" version="{Shuttle.Core.Container-version}" />
        <dependency id="Shuttle.Core.Uris" version="{Shuttle.Core.Uris-version}" />
        <dependency id="System.Configuration.ConfigurationManager" version="{System.Configuration.ConfigurationManager-version}" />
      </group>
    </dependencies>
  </metadata>
</package>